<% cache document do %>
  <%= link_to airline_fleet_path(document.airline, document) do %>
    <div class="search-result">
      <div class="search-result__icon">
        <%= image_tag "icons/fleet_icon.png" %>
      </div>
      <div class="search-result__content">
        <span class="search-result__heading"><%= document.name %></span>
        <span class="search-result__data"><%= document.install_type %></span>
      </div>
    </div>
  <% end %>
<% end %>