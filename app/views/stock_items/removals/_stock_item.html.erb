<% cache stock_item do %>
  <tr>
    <td><input id="checkBox" type="checkbox"></td>
    <td><%= l stock_item.transfers.last.updated_at, format: :simple %></td>
    <td><%= stock_item.product.name %></td>
    <td><%= stock_item.product.part_number %></td>
    <td><%= stock_item.serial_number %></td>
    <td><%= stock_item.model_numbers %></td>
    <td><%= stock_item.revision %></td>
    <td><%= link_to 'Edit', edit_stock_item_path(stock_item, {next_step: "create_certificate"}) %></td>
  </tr>
<% end %>
