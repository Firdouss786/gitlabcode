<% cache stock_item do %>
  <tr>
    <td class=""><input id="checkBox" type="checkbox"></td>
    <td class=""><%= stock_item.transfers.last.sent_at %></td>
    <td class=""><%= stock_item.product.name %></td>
    <td class=""><%= stock_item.product.part_number %></td>
    <td class=""><%= stock_item.serial_number %></td>
    <td class=""><%= stock_item.transfers.last.comment %></td>
  </tr>
<% end %>
